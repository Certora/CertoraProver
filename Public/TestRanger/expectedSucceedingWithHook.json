{
	"rules": {
		"succeedingWithHook": {
			"Initial State": "SUCCESS",
			"Range 1": {
				"SUCCESS": [
					"succeedingWithHook-Initial State-InvParams.setFoo()|InvParams.setFooConditionally((uint256))"
				]
			},
			"Range 2": {
				"SUCCESS": [
					"succeedingWithHook-Initial State-InvParams.setBar(uint256)-InvParams.setFooConditionally((uint256))",
					"succeedingWithHook-Initial State-InvParams.setFoo()-InvParams.setFooConditionally((uint256))",
					"succeedingWithHook-Initial State-InvParams.setFooConditionally((uint256))-InvParams.setFoo()"
				]
			}
		}
	}
}
